{
  "name": "test-git",
  "version": "1.0.0",
  "description": "Nuestro primer proyecto de Node.js.",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "clean": "rimraf dist",
    "imagemin": "imagemin img/* --out-dir dist/img"
  },
  "repository": {
    "type": "git",
    "url": "git+https://moiserma@bitbucket.org/moiserma/test-git.git"
  },
  "author": "Moises Serrano Marin",
  "license": "ISC",
  "homepage": "https://bitbucket.org/moiserma/test-git#readme",
  "devDependencies": {
    "copyfiles": "^2.3.0",
    "lite-server": "^2.5.4",
    "node-sass": "^4.14.1",
    "onchange": "^7.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.26.10"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "concurrently": "^5.3.0",
    "jquery": "^3.5.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1"
  }
}
